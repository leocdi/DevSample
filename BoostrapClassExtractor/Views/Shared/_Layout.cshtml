<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BoostrapClassExtractor</title>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css" />
    @*    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/BoostrapClassExtractor.styles.css" asp-append-version="true" />*@
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="@Bs5.navbar_brand" asp-area="" asp-controller="Home" asp-action="Index">BoostrapClassExtractor</a>
                <button class="@Bs5.navbar_toggler" type="button" data-bs-toggle="collapse" data-bs-target=".@Bs5.navbar_collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="@Bs5.navbar_toggler_icon"></span>
                </button>
                <div class="@Bs5.navbar_collapse collapse d-sm-inline-flex @Bs5.justify_content_between">
                    <ul class="@Bs5.navbar_nav flex-grow-1">
                        <li class="nav-item">
                            <a class="@Bs5.nav_link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="@Bs5.nav_link @Bs5.text_dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="@Bs5.pb_3">
            @RenderBody()
        </main>
    </div>

    <footer class="@Bs5.border_top footer @Bs5.text_muted">
        <div class="container">
            &copy; 2023 - BoostrapClassExtractor - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
